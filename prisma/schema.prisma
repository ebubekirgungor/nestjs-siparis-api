generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model products {
  id             Int    @id @default(autoincrement())
  title          String
  category_id    Int
  author         String
  list_price     Float
  stock_quantity Int
}

model categories {
  id    Int    @id @default(autoincrement())
  title String
}
